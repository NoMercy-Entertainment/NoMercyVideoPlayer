{
	"compilerOptions": {
		"target": "ES2016",
		"module": "es6",
		"outDir": "./dist",
		// "outFile": "./build/build.js",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"lib": ["es5", "es6", "dom", "dom.iterable"],
		"allowJs": true,
		"jsx": "react",
		"sourceMap": true,
		"rootDir": ".",
		"downlevelIteration": true,
		"moduleResolution": "Node",
		"declaration": true,
		"types": ["node", "jest"]
	},
	"include": ["./src/**/*.ts", "./src/**/*.d.ts", "src/*.d.ts", "*.d.ts", "src/app.js", "./vite.config.ts", "./index.js"]
}
